{
  "name": "phytec/rhea-reports",
  "description": "Generate report pdfs for rhea test runs.",
  "type": "yii2-extension",
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Frederik Lux",
      "email": "f.lux@phytec.de"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0",
    "yiisoft/yii2": ">=2.0.4",
    "setasign/fpdf": "^1.8.6"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
